<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             xmlns:ui="using:FluentAvalonia.UI.Controls"
			 x:Class="XmlToExcel.Views.LoadingView">


	<Grid Opacity=".6" Background="White">
		<Border Background="Black" CornerRadius="25" Padding="10" HorizontalAlignment="Center" VerticalAlignment="Center">
			<ui:ProgressRing IsIndeterminate="True" BorderThickness="10" Width="100" Height="100" Foreground=""/>
		</Border>
	</Grid>
</UserControl>
